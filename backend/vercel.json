{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@MONGODB_URI",
      "JWT_SECRET": "@JWT_SECRET"
    }
  }